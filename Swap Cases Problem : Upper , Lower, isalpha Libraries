def swap_case(s):
    string = ""
    for i in range(len(s)):
        if s[i].isupper() and s[i].isalpha:
            string += s[i].lower()
        elif s[i].islower()and s[i].isalpha:
            string += s[i].upper()
        elif s[i].isalpha() is False or s[i] == ' ':
            string +=s[i]
            
    return string

if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)
